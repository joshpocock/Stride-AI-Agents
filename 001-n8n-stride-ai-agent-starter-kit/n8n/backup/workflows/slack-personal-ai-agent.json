                "maxConnections": 1,
                "required": true
              },
              {
                "type": "ai_embedding",
                "maxConnections": 1,
                "required": true
              }
            ]
          },
          "outputs": {
            "output": [
              {
                "type": "main"
              }
            ]
          }
        },
